- if @registrations.any?
  - @registrations.each do |registration|
    %tr
      %td= link_to registration.full_name, admin_registration_path(registration)
      %td= registration.email
      %td= registration.city
      %td= registration.created_at.strftime("%B %d, %Y at %H:%M")
      %td
        = link_to t('.edit', :default => t("helpers.links.edit")), edit_admin_registration_path(registration), :class => 'btn btn-default btn-xs'
        = link_to t('.destroy', :default => t("helpers.links.destroy")), admin_registration_path(registration), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs btn-danger'
- else
  %p.text-center.text-muted
    No results found